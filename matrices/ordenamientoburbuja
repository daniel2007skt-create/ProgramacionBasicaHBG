#include <stdio.h>

int main() {
    int N;
    int i;
    int j;
    int aux;

    printf("Ingrese el tamano del vector: ");
    scanf("%d", &N);

    int vector[N];

    for(i = 0; i < N; i++) {
        printf("Elemento %d: ", i);
        scanf("%d", &vector[i]);
    }

    for(i = 0; i < N - 1; i++) {
        for(j = 0; j < N - 1 - i; j++) {
            if(vector[j] > vector[j + 1]) {
                aux = vector[j];
                vector[j] = vector[j + 1];
                vector[j + 1] = aux;
            }
        }
    }

    for(i = 0; i < N; i++) {
        printf("%d\n", vector[i]);
    }

    return 0;
}
